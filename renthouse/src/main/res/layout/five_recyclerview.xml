<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="horizontal"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:paddingBottom="20dp"
    >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <ImageView
            android:id="@+id/shouye_five_iv_imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/background"
            />
        <LinearLayout
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:orientation="horizontal"
          android:gravity="center"
            >
            <ImageView
                android:id="@+id/shouye_five_iv_icon"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:src="@mipmap/ic_launcher_round"
                android:visibility="gone"
                />
            <TextView
                android:id="@+id/shouye_five_text_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="发布人姓名"
                android:layout_marginLeft="5dp"
                android:visibility="gone"
                android:textColor="@color/tab_textColor"
                />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="5dp"
        >
    <TextView
        android:id="@+id/shouye_five_text_whole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        style="@style/MyCustomTabLayout"
        android:textColor="@drawable/textselector"
        android:text="整租-乐山二村"
        android:layout_gravity="center_horizontal"
        />


    <TextView
        android:id="@+id/shouye_five_text_oneroom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        style="@style/MyCustomTabLayout"
        android:textColor="@drawable/textselector"
        android:text="一室一厅"
        android:layout_gravity="center_horizontal"
        android:paddingLeft="5dp"
        />
        </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <ImageView
            android:layout_width="27dp"
            android:layout_height="match_parent"
            android:src="@mipmap/star"
            />
        <TextView
            android:id="@+id/shouye_five_text_starNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            style="@style/MyCustomTabLayout"
            android:textColor="@drawable/textselector"
            android:text="5.0 |"
            android:layout_gravity="center_horizontal"
            />


        <TextView
            android:id="@+id/shouye_five_text_oneroom2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            style="@style/MyCustomTabLayout"
            android:textColor="@drawable/textselector"
            android:text="1室1厅1卫 |"
            android:layout_gravity="center_horizontal"
            android:paddingLeft="5dp"
            />
        <TextView
            android:id="@+id/shouye_five_text_landlord"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            style="@style/MyCustomTabLayout"
            android:textColor="@drawable/textselector"
            android:text="职业房东"
            android:layout_gravity="center_horizontal"
            android:paddingLeft="5dp"/>

    </LinearLayout>
        <TextView
            android:id="@+id/shouye_five_text_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="地址：xxxxxxxxxxxxxxx"
            android:paddingLeft="5dp"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="5dp"
            >
            <TextView
                android:id="@+id/shouye_five_text_label1"
                android:layout_width="50dp"
                android:layout_height="25dp"
                android:background="@color/tab_textColorSelect"
                android:text="标签"
                android:textColor="@color/tab_background"
                android:gravity="center"
                android:layout_marginRight="5dp"
                />

            <TextView
                android:id="@+id/shouye_five_text_label2"
                android:layout_width="50dp"
                android:layout_height="25dp"
                android:background="@color/tab_textColorSelect"
                android:text="标签"
                android:textColor="@color/tab_background"
                android:gravity="center"
                android:layout_marginRight="5dp"
                />

            <TextView
                android:id="@+id/shouye_five_text_label3"
                android:layout_width="50dp"
                android:layout_height="25dp"
                android:background="@color/tab_textColorSelect"
                android:text="标签"
                android:textColor="@color/tab_background"
                android:gravity="center"
                android:layout_marginRight="5dp"
                />

        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <TextView
                android:id="@+id/shouye_five_text_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                style="@style/MyCustomTabLayout"
                android:textColor="#FF0000"
                android:text="3500元/月"
                android:layout_marginLeft="5dp"
                />
            <TextView
                android:id="@+id/shouye_five_text_updatetime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                style="@style/MyCustomTabLayout"
                android:textColor="#FF0000"
                android:text="一小时前更新"
                android:layout_alignParentRight="true"
                android:visibility="gone"
                />
        </RelativeLayout>


    </LinearLayout>
</LinearLayout>